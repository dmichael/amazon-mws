						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="PackageType" minOccurs="0">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="body-only"/>
							<xsd:enumeration value="multiple-lenses"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="Remote" type="LongString" minOccurs="0"/>
				<xsd:element name="WaterResistanceDepth" type="LengthDimension" minOccurs="0"/>
				<xsd:element name="WaterResistanceLevel" type="WaterResistantType" minOccurs="0"/>
			</xsd:sequence>
		</xsd:complexType>
